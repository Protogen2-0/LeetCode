function myAtoi(s: string): number {
    let str = ""
    for(let i = 0; i < s.length; i++){
        let symbol = s[i]
        if(symbol === " ") {
            if(str.length){break}
            continue
        }
        if ( !(str.length == 0 && (symbol == "+" || symbol == "-")) && isNaN(+symbol) ){break}
        str += symbol
    }
    if (+str > 2**31 - 1) {return 2**31 - 1}
    if (+str < -(2**31)) {return -(2**31)}
    return isNaN(+str) ? 0 : +str
};
