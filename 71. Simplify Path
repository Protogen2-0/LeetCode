function simplifyPath(path: string): string {
    while(path.includes("//")){
        path = path.replaceAll("//", "/")
    }
    while(path.includes("/./")){
        path = path.replace("/./", "/")
    }
    if(path.endsWith("/")) path = path.substr(0, path.length - 1)
    if(path.endsWith("/.")) path = path.substr(0, path.length - 2)
    let arr = path.split("/")
    let count = 0
    for(let i = 0; i < arr.length; i++){
        if(arr[i] === ".."){
            arr[i] = ""
            while(arr[i - 1 - count] === ""){
                count++
            }
            arr[i - 1 - count] = ""
            continue
        }
        count = 0
    }
    arr = arr.filter( (e) => {
        return e !== ""
    })
    let newArr = []
    for(let i = 0; i < arr.length; i++){
        newArr.push("/")
        newArr.push(arr[i])
    }
    if(!newArr.length) newArr.push("/")
    return newArr.join("")
};
