function reverse(x: number): number {
    let reversed: number
    if (x < 0) {
        reversed = +((x.toString()).substr(1, x.toString().length - 1).split("").reverse().join(""))
        if (-reversed < -(2**31)) {return 0}
        return reversed * -1
    }
    reversed = +(x.toString().split("").reverse().join(""))
    if (reversed > 2**31 - 1) {return 0}
    return reversed
};
