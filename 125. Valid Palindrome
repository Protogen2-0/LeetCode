function isPalindrome(s: string): boolean {
    s = s.toLowerCase()
    s = s.replaceAll(" ", "")
    s = s.replaceAll(",", "")
    s = s.replaceAll(":", "")
    s = s.replaceAll("_", "")
    s = s.replaceAll(".", "")
    s = s.replaceAll("\/", "")
    s = s.replaceAll("\\", "")
    s = s.replaceAll("@", "")
    s = s.replaceAll("#", "")
    s = s.replaceAll("-", "")
    s = s.replaceAll("(", "")
    s = s.replaceAll(")", "")
    s = s.replaceAll("[", "")
    s = s.replaceAll("]", "")
    s = s.replaceAll("\"", "")
    s = s.replaceAll("\'", "")
    s = s.replaceAll("{", "")
    s = s.replaceAll("}", "")
    s = s.replaceAll("?", "")
    s = s.replaceAll("!", "")
    s = s.replaceAll(";", "")
    s = s.replaceAll("\`", "")
    return s.split('').reverse().join('') === s
};
