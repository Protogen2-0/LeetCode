var removeElement = function(nums, val) {
    let count = 0
    let arr = nums
    for(let i = 0; i < arr.length; i++){
        if(arr[i] !== val){
            count++
            continue
        }
        arr[i] = "_"
    }
    arr = arr.sort()
    for(let i = 0; i < arr.length; i++){
        nums[i] = arr[i]
    }
    return count
};
