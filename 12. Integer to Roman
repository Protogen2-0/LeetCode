function intToRoman(num: number): string {
    let str = ""
    while(num >= 1000){
        str += "M"
        num -= 1000
    }
    // [0,999]
    if ( num >= 900 ) {
        str += "CM"
        num -= 900
    }
    //[0,899]
    if ( num >= 500 ) {
        str += "D"
        num -= 500
    }
    // [0,499]
    if ( num >= 400) {
        str += "CD"
        num -= 400
    }
    // [0,399]
    while(num >= 100){
        str += "C"
        num -= 100
    }
    // [0,99]
    if ( num >= 90 ) {
        str += "XC"
        num -= 90
    }
    //[0,89]
    if ( num >= 50 ) {
        str += "L"
        num -= 50
    }
    // [0, 49]
    if ( num >= 40 ) {
        str += "XL"
        num -= 40
    }
    // [0,39]
    while(num >= 10){
        str += "X"
        num -= 10
    }
    // [0,9]
    if ( num >= 9 ) {
        str += "IX"
        num -= 9
    }
    //[0,8]
    if ( num >= 5 ) {
        str += "V"
        num -= 5
    }
    // [0,4]
    if ( num >= 4 ) {
        str += "IV"
        num -= 4
    }
    // [0,3]
    while(num){
        str += "I"
        num--
    }
    return str
};
